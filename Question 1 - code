library(readxl)

mSales = read_excel("Percent of Retail and Food (Monthly).xlsx")
mSales$USREC = ifelse(mSales$USREC == 1, "Yes", "No")

# All other gen. merchandise stores 1
t.test(`All other gen. merchandise stores` ~ USREC, data = mSales)
boxplot(mSales$`All other gen. merchandise stores` ~mSales$USREC, xlab="US Recession",
        ylab="% of Total Sales", main = "All other gen. merchandise stores")

# Automobile and other motor vehicle dealers 2
t.test(`Automobile and other motor vehicle dealers` ~ USREC, data = mSales)
boxplot(mSales$`Automobile and other motor vehicle dealers` ~mSales$USREC, xlab="US Recession",
        ylab="% of Total Sales", main = "Automobile and other motor vehicle dealers")

# Automotive parts, acc., and tire stores 3
t.test(`Automotive parts, acc., and tire stores` ~ USREC, data = mSales)
boxplot(mSales$`Automotive parts, acc., and tire stores` ~mSales$USREC, xlab="US Recession",
        ylab="% of Total Sales", main = "Automotive parts, acc., and tire stores")

# Beer, wine and liquor stores 4
t.test(`Beer, wine and liquor stores` ~ USREC, data = mSales)
boxplot(mSales$`Beer, wine and liquor stores` ~mSales$USREC, xlab="US Recession",
        ylab="% of Total Sales", main = "Beer, wine and liquor stores")

# Building mat. and garden equip. and supplies dealers 5
